import "io"

enum Event {
  Keyboard(str)
  Click(num, num)
  Close
}

const events arr = [
  Event.Click(123, 321),
  Event.Keyboard("k"),
  Event.Close
]

for mut i num = 0, ~events - 1 {
  match events[i] {
    Event.Keyboard |key| {
      io::printLn("keyboard event with key:", key)
    }
    Event.Click |x, y| {
      io::printLn("click event at x:", x, "y:", y)
    }
    Event.Close {
      io::printLn("close event")
    }
  }
}


